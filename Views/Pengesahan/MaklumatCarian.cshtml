                                <th class="text-center">@Resources.Verification.VV_Apply</th>
                                <th class="text-center">@Resources.Verification.VV_Review</th>
                                <th class="text-center">@Resources.Verification.VV_Approval</th>
                                <th class="text-center">@Resources.Resource._Status</th>
