        <!-- END PEMEGANG TAB -->
        <!-- START KUATKUASA TAB -->
        <div class="tab-pane fade show active" id="kuatkuasa" role="tabpanel" aria-labelledby="kuatkuasa-tab">
            <div class="col-xl-12 col-lg-12 col-md-12 mx-auto">
                <div class="table-responsive">
                    <table class="table table-bordered table-hover mb-4" id="lesenKuatkuasa">
                        <thead>
                            <tr>
                                <th class="text-center">@Resources.Verification.VV_No</th>
                                <th class="text-center" width="20%">@Resources.Verification.VV_LicenseType</th>
                                <th class="text-center" width="20%">@Resources.Verification.VV_OfficeProducer</th>
                                <th class="text-center" width="10%">@Resources.Verification.VV_StartDate</th>
                                <th class="text-center" width="10%">@Resources.Verification.VV_EndDate</th>
                                <th class="text-center" width="20%">@Resources.Verification.VV_EntryDate</th>
                                <th class="text-center">@Resources.Resource._Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            @{int no = 1; }
                            @foreach (var m in Model.getKuatkuasaLesen)
                            {
                                <tr>
                                    <td class="text-center">@no</td>
                                    <td class="text-center">@m.NamaLesen [@m.NoLesen]</td>
                                    <td class="text-center">@m.PejabatPengeluar</td>
                                    <td class="text-center">
                                        @Convert.ToDateTime(m.TarikhMula).ToString("dd/MM/yyyy hh:mm tt", new System.Globalization.CultureInfo("en-US"))
                                    </td>
                                    <td class="text-center">
                                        @Convert.ToDateTime(m.TarikhTamat).ToString("dd/MM/yyyy hh:mm tt", new System.Globalization.CultureInfo("en-US"))
                                    </td>
                                    <td class="text-center">-</td>
                                    <td class="text-center">
                                        <a href="#" onclick="openNewTab('@Url.Action("ViewSemakanLesen", "Pengesahan",new {id = GlobalFunction.encodeThis(m.LesenID.ToString()) })')"><i class="feather-20" data-feather="edit"></i></a>
                                    </td>
                                </tr>
                                no++;
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <!-- END KUATKUASA TAB -->
        <!-- START LESENTAMAT TAB -->
        <div class="tab-pane fade" id="lesentamat" role="tabpanel" aria-labelledby="lesentamat-tab">
            <div class="col-xl-12 col-lg-12 col-md-12 mx-auto">
                <div class="table-responsive">
                    <table class="table table-bordered table-hover mb-4" id="lesenTamat">
                        <thead>
                            <tr>
                                <th class="text-center" width="5%">@Resources.Verification.VV_No</th>
                                <th class="text-center">@Resources.Verification.VV_LicenseType</th>
                                <th class="text-center">@Resources.Verification.VV_OfficeProducer</th>
                                <th class="text-center">@Resources.Verification.VV_LicenseNo</th>
                                <th class="text-center">@Resources.Verification.VV_StartDate</th>
                                <th class="text-center">@Resources.Verification.VV_EndDate</th>
                                <th class="text-center">@Resources.Resource._Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            @{int no1 = 1; }
                            @foreach (var m in Model.getTamatTempohLesen)
                            {
                                <tr>
                                    <td class="text-center" width="5%">@no1</td>
                                    <td class="text-center">@m.NamaLesen</td>
                                    <td class="text-center">@m.PejabatPengeluar</td>
                                    <td class="text-center">@m.NoLesen</td>
                                    <td class="text-center" width="15%">
                                        @*@m.TarikhMula*@
                                        @Convert.ToDateTime(m.TarikhMula).ToString("dd/MM/yyyy hh:mm tt", new System.Globalization.CultureInfo("en-US"))
                                    </td>
                                    <td class="text-center" width="15%">
                                        @Convert.ToDateTime(m.TarikhTamat).ToString("dd/MM/yyyy hh:mm tt", new System.Globalization.CultureInfo("en-US"))
                                    </td>
                                    @*Module selain V&V*@
                                    <td class="text-center">
                                        <a href="#" onclick="openNewTab('@Url.Action("ViewSemakanLesen", "Pengesahan",new {id = GlobalFunction.encodeThis(m.LesenID.ToString()) })')"><i class="feather-20" data-feather="edit"></i></a>
                                    </td>

                                </tr>
                                no1++;
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
