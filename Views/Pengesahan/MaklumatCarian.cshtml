        <div class="tab-pane fade" id="digantung" role="tabpanel" aria-labelledby="digantung-tab">
            <div class="col-xl-12 col-lg-12 col-md-12 mx-auto">
                <div class="table-responsive">
                    <table class="table table-bordered table-hover mb-4" id="lesenDigantung">
                        <thead>
                            <tr>
                                <th class="text-center">@Resources.Verification.VV_No</th>
                                <th class="text-center" width="28%">@Resources.Verification.VV_LicenseType [@Resources.Verification.VV_LicenseNo]</th>
                                <th class="text-center">@Resources.Verification.VV_OfficeProducer</th>
                                @*<th class="text-center">@Resources.Verification.VV_LicenseNo</th>*@
                                <th class="text-center">@Resources.Verification.VV_StartDate</th>
                                <th class="text-center">@Resources.Verification.VV_EndDate</th>
                                <th class="text-center">@Resources.Resource._Status</th>
                                @*<th class="text-center" width="15%">@Resources.Verification.VV_Notes</th>*@
                            </tr>
                        </thead>
                        <tbody>
                            @{int no3 = 1; }
                            @foreach (var m in Model.getStatusBatalLesen)
                            {
                                <tr>
                                    <td class="text-center">@no3</td>
                                    <td class="text-center" width="28%">@m.NamaLesen [@m.NoLesen]</td>
                                    <td class="text-center" width="19%">@m.PejabatPengeluar</td>
                                    <td class="text-center" width="15%">
                                        @*@m.TarikhMula*@
                                        @Convert.ToDateTime(m.TarikhMula).ToString("dd/MM/yyyy hh:mm tt", new System.Globalization.CultureInfo("en-US"))
                                    </td>
                                    <td class="text-center" width="15%">
                                        @Convert.ToDateTime(m.TarikhTamat).ToString("dd/MM/yyyy hh:mm tt", new System.Globalization.CultureInfo("en-US"))
                                    </td>
                                    <td class="text-center">
                                        <span class="badge badge-secondary">@m.Status </span>
                                    </td>
                                    @*<td class="text-center" width="15%">@m.Catatan</td>*@
                                </tr>
                                no3++;
                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <!-- END DIGANTUNG TAB -->
        @*</div>*@
        <!-- END TAB -->
    </div>
</div>

