@section scripts{
    <script>
        $(document).ready(function () {
            //formatted date using flatpickr
            $("#date").flatpickr({
                dateFormat: "d-m-Y",
            });

            $("#dateTime").flatpickr({
                enableTime: true,
                dateFormat: "d-m-Y H:i K",
            });

            //to get the value from select option
            $("#appendiks").change(function () {
                var appendik = $(this).val();

                //assign the appendiks to the new appendik input type based on class(because input type>1)
                $('.newAppendiks').val(appendik);

            });

            //to get and assign for 'sumber' input type
            $("#sumber").change(function () {
                var sumber = $(this).val();

                //assign to new sumber input type
                $(".newSumber").val(sumber);
            });

            //display success message
            function message() {
               return swal({
                    //title: 'Saved!',
                    text: "@Resources.Verification.VV_AlertSaved",
                    type: "success",
                    padding: '2em'
                })
            }

            //Start Enable and Disable Tabs
            $('#lesen').on("click", function () {
                message();
                $('#v-pills-noPermit-tab').removeClass("disabled");
                $('#v-pills-noPermit-tab').removeClass("no-drop");
                $('#v-pills-noPermit-tab').attr('data-toggle', 'pill');
                $('.nav-pills> .active').next('a').trigger('click');
                $('#v-pills-jenisLesen-tab').addClass("changeTabWhite");
            });

            $('#noPermit').on("click", function () {
                message();
                $('#v-pills-pemilik-tab').removeClass("disabled");
                $('#v-pills-pemilik-tab').removeClass("no-drop");
                $('#v-pills-pemilik-tab').attr('data-toggle', 'pill');
                $('.nav-pills> .active').next('a').trigger('click');
            });

            $('#pemilik').on("click", function () {
                message();
                $('#v-pills-syarikat-tab').removeClass("disabled");
                $('#v-pills-syarikat-tab').removeClass("no-drop");
                $('#v-pills-syarikat-tab').attr('data-toggle', 'pill');
                $('.nav-pills> .active').next('a').trigger('click');
            });

            $('#syarikat').on("click", function () {
                message();
