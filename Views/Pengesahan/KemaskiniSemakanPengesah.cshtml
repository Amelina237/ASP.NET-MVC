                $('#v-pills-pemeriksaan-tab').attr('data-toggle', 'pill');
                $('.nav-pills> .active').next('a').trigger('click');
            });

            $('#pemeriksaan').on("click", function () {
                message();
                $('#v-pills-lampiran-tab').removeClass("disabled");
                $('#v-pills-lampiran-tab').removeClass("no-drop");
                $('#v-pills-lampiran-tab').attr('data-toggle', 'pill');
                $('.nav-pills> .active').next('a').trigger('click');
