@model ePerhilitanV2.Models.ViewModels.PengesahanViewModel.ImportExport

<div class="form-card" style="padding: 20px">
    <div class="col-xl-12 col-lg-12 col-md-12 mx-auto">
        <div class="col-xl-12 col-lg-12 col-md-12 mx-auto">
            <div class="table-responsive">
                <table class="table table2 table-bordered" id="">
                    <thead>
                        <tr class="thead-light">
                            <th colspan="8">@Resources.Verification.VV_Cites_Info</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.License_Type == "Import")
                        {
                            <tr>
                                <td>
                                    <div class="form-group row">
                                        <label for="colFormLabelSm" class="col-sm-3 col-form-label col-form-label-sm">@Resources.Verification.VV_ExportCountry</label>
                                        <div class="col-sm-9">
                                            @if (Model.Negara_Eksport != null)
                                            {
                                                @Model.Negara_Eksport.ToUpper()
                                            }
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-group row">
                                        <label for="colFormLabelSm" class="col-sm-3 col-form-label col-form-label-sm">@Resources.Verification.VV_ExportPurpose</label>
                                        <div class="col-sm-9">
                                            @Model.Tujuan
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-group row">
                                        <label for="colFormLabelSm" class="col-sm-3 col-form-label col-form-label-sm">@Resources.Verification.VV_ExportName</label>
                                        <div class="col-sm-9">
                                            @if (Model.Nama_Eksport != null)
                                            {
                                                @Model.Nama_Eksport.ToUpper()
                                            }
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-group row">
                                        <label for="colFormLabelSm" class="col-sm-3 col-form-label col-form-label-sm">@Resources.Verification.VV_ExportAddress</label>
                                        <div class="col-sm-6">
                                            @if (Model.Alamat_Pengeksport != null)
                                            {
                                                @Model.Alamat_Pengeksport.ToUpper()
                                            }
                                        </div>
                                    </div>
                                </td>
                            </tr>

                        }
                        else if (Model.License_Type == "Eksport" || Model.License_Type == "Eksport Semula")
                        {
                            <tr>
                                <td>
                                    <div class="form-group row">
                                        <label for="colFormLabelSm" class="col-sm-3 col-form-label col-form-label-sm">@Resources.Verification.VV_ImportCountry</label>
                                        <div class="col-sm-9">
                                            @if (Model.Negara_Eksport != null)
                                            {
                                                @Model.Negara_Import.ToUpper()
                                            }
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-group row">
                                        <label for="colFormLabelSm" class="col-sm-3 col-form-label col-form-label-sm">@Resources.Verification.VV_ImportPurpose</label>
                                        <div class="col-sm-9">
                                            @Model.Tujuan
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-group row">
                                        <label for="colFormLabelSm" class="col-sm-3 col-form-label col-form-label-sm">@Resources.Verification.VV_ImporterName</label>
                                        <div class="col-sm-9">
                                            @if (Model.Nama_Import != null)
                                            {
                                                @Model.Nama_Import.ToUpper()
                                            }
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-group row">
                                        <label for="colFormLabelSm" class="col-sm-3 col-form-label col-form-label-sm">@Resources.Verification.VV_ImportCountry</label>
                                        <div class="col-sm-6">
                                            @if (Model.Alamat_Pengimport != null)
                                            {
                                                @Model.Alamat_Pengimport.ToUpper()
                                            }
                                        </div>
                                    </div>
                                </td>
                            </tr>
                        }
                        <tr>
                            <td>
                                <div class="form-group row">
                                    <label for="colFormLabelSm" class="col-sm-3 col-form-label col-form-label-sm">@Resources.Verification.VV_StemNo</label>
                                    <div class="col-sm-9">
                                        @Model.No_Stem
                                    </div>
                                </div>
                            </td>
                        </tr>

                        @foreach (var d in Model.getListDocument)
                        {
                            if (d.File_Name == "maklumat_cites")
                            {
                                <tr>
                                    <td>
                                        <div class="form-group row">
                                            <label for="colFormLabelSm" class="col-sm-3 col-form-label col-form-label-sm">@Resources.Verification.VV_Copy_Cites</label>
                                            <div class="col-sm-6 form-control-sm">
                                                @*<a href="~/Content/@d.File_Location" target="_blank"> <i class="feather-20" data-feather="download"></i>@d.File_Name </a>*@
                                                <a href="#" onclick="openNewTab('/Content/@d.File_Location'); return false;"><i class="feather-20" data-feather="download"></i> @d.File_Name </a>
                                            </div>
                                        </div>
                                    </td>
                                </tr>
                            }

                            if (d.File_Name == "maklumat_cites_ubah")
                            {
                                <tr>
                                    <td>
                                        <div class="form-group row">
                                            <label for="colFormLabelSm" class="col-sm-3 col-form-label col-form-label-sm">@Resources.Verification.VV_CitesChanges</label>
                                            <div class="col-sm-6 form-control-sm">
                                                <a href="#" onclick="openNewTab('/Content/@d.File_Location'); return false;"><i class="feather-20" data-feather="download"></i> @d.File_Name </a>
                                            </div>
                                        </div>
                                    </td>
                                </tr>
                            }
                        }

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>


